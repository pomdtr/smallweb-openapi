{
    "name": "@smallweb/openapi",
    "version": "0.14.8",
    "exports": {
        ".": "./mod.ts"
    },
    "license": "MIT",
    "fmt": {
        "indentWidth": 2
    },
    "tasks": {
        "refresh": "deno run -A tasks/refresh.ts",
        "publish-npm": "deno run -A tasks/npm.ts && cd npm && npm publish",
        "publish-jsr": "deno publish",
        "publish": "deno run publish-jsr && deno run publish-npm"
    },
    "publish": {
        "include": [
            "README.md",
            "mod.ts",
            "openapi.json"
        ]
    }
}
